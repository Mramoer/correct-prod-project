{
	"presets": [
		"@babel/preset-env", // Пресет для работы с современными версиями JavaScript
		"@babel/preset-react",
		"@babel/preset-typescript"
	],
	"plugins": [
		[
			"i18next-extract",
			{ "locales": ["ru", "en"], "keyAsDefaultValue": true } // Опции для извлечения ключей локализации
		]
	]
}
